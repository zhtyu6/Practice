import java.util.Scanner;

public class Main1018 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	while(n-->0) {
		int m=sc.nextInt();
		double sum=0;int ans;
		for(int i=1;i<=m;i++) {
			sum+=Math.log10(i);
		}
		ans=(int) sum+1;
		System.out.println(ans);
	}
}
}
